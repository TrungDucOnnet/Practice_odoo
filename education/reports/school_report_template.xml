<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="school_report_template">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="web.external_layout">
                    <div class="page">
                        <h2>School Report</h2>
                        <p>School Name: <span><t t-out = "o.name"/></span></p>
                    </div>
                </t>
            </t>
        </t>
    </template>
</odoo>